olor: white;\r\n    //animation: name duration timing-function delay iteration-count direction fill-mode;\r\n    // animation: textStyle 4s ease-in-out infinite;\r\n    // -webkit-animation: textStyle 4s ease-in-out infinite;\r\n}\r\n\r\n@keyframes textStyle {\r\n    to {\r\n        background: linear-gradient(to right, pink, burlywood, purple);\r\n        -webkit-text-fill-color: transparent;\r\n        background-clip: text;\r\n        -webkit-background-clip: text;\r\n    }\r\n    from {        \r\n        background: linear-gradient(to right, orange, white, black);\r\n        -webkit-text-fill-color: transparent;\r\n        background-clip: text;\r\n        -webkit-background-clip: text;\r\n    }\r\n}\r\n\r\nbutton {\r\n    background-color: transparent;\r\n    width: 120px;\r\n    height: 40px;\r\n    border: 2px solid;\r\n    color: inherit;\r\n    margin-top: .5em;\r\n}","..\\sass\\abstract\\_mixins.scss":"//\r\n\r\n// ul, li mixin\r\n\r\n@mixin list-reset($list) {\r\n    @if $list == \"reset-list\" {\r\n    margin: 0;\r\n    padding: 0;\r\n    list-style: none;\r\n    }\r\n}\r\n\r\n//\r\n// center\r\n\r\n@mixin positionCenter($center) {\r\n    @if $center == absoluteCenter {\r\n        position: absolute;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%, -50%);\r\n        -webkit-transform: translate(-50%, -50%);\r\n        -moz-transform: translate(-50%, -50%);\r\n        -ms-transform: translate(-50%, -50%);\r\n        -o-transform: translate(-50%, -50%);\r\n    }    \r\n    @else if $center == verticalCenter {\r\n        position: absolute;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(0, 50%);\r\n        -webkit-transform: translate(0, 50%);\r\n        -moz-transform: translate(0, 50%);\r\n        -ms-transform: translate(0, 50%);\r\n        -o-transform: translate(0, 50%);\r\n}\r\n    @else if $center == gridCenter {\r\n        display: grid;\r\n        place-content: center;\r\n    }\r\n    @else if $center == flexCenter {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n}\r\n","..\\sass\\abstract\\_breakpoints.scss":"\r\n@mixin breakpoint($point) {\r\n    @if $point == small {\r\n        @media screen and (max-device-width: 480px)\r\n        and\r\n        (orientation: landscape) {\r\n            @content;\r\n        }\r\n    }\r\n    @else if $point == medium {\r\n        @media screen and (min-device-width: 768px)\r\n        and\r\n        (max-device-width: 1024px) {\r\n            @content;\r\n        }\r\n    }\r\n    @else if $point == large {\r\n        @media screen and (min-width: 1024px) {\r\n            @content;\r\n        }\r\n    }\r\n}\r\n\r\n// @mixin breakpoint($point) {\r\n//     switch ($point) {\r\n//         case 1 {\r\n\r\n//         }\r\n//     }\r\n// }","..\\sass\\utilities\\_effects.scss":"// blurry bg\r\n\r\n.blurry-bg {\r\n    backdrop-filter: blur(2px);\r\n}\r\n","..\\sass\\utilities\\_forms.scss":"//\r\n\r\n@mixin inputText($input) {\r\n    \r\n}\r\n\r\n.input-pairs {\r\n    position: relative;\r\n    input {\r\n        @extend .input-pairs;\r\n        padding: .4em;\r\n        margin-block: .2em;\r\n        width: 15em;\r\n    }\r\n}\r\n\r\n.input-label {\r\n    position: absolute;\r\n    inset: 0;\r\n    top: .6999em;\r\n    left: .5em;\r\n    pointer-events: none;\r\n}\r\n\r\n","..\\sass\\layout\\_header.scss":"//\r\n// @use \"../\";\r\n@use \"../abstract\" as *;\r\n@use \"../components/\" as *;\r\n@use \"../utilities/\" as *;\r\n\r\n.mobile-nav {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    border-bottom: 2px solid white;\r\n}\r\n.logo {\r\n    padding-left: .5em;\r\n    border: 2px solid black;\r\n}\r\nheader {\r\n    position: relative;\r\n    @include breakpoint(small) {\r\n    ul {\r\n        @include list-reset(reset-list);\r\n        text-align: center;\r\n        position: absolute;\r\n        top: 40px;\r